    <androidx.constraintlayout.widget.ConstraintLayout
        class="androidx.appcompat.widget.Toolbar"
        android:id="@+id/topbar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/menubackground"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/currentcolor"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:background="@drawable/whitecircle"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>

        <TextView
            android:id="@+id/player1"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="10dp"
            android:textSize="30sp"
            android:textAlignment="center"/>


        <TextView
            android:id="@+id/player2"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="10dp"
            android:textSize="30sp"
            android:textAlignment="center"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <GameCore.Graphics.SelectView
        android:id="@+id/selector"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/bottommenu">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff">

             <GameCore.SelectButton
                android:id="@+id/queenselect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/queenwhite"
                 app:layout_constraintTop_toTopOf="parent"
                 app:layout_constraintLeft_toLeftOf="parent"
                />

            <GameCore.SelectButton
                android:id="@+id/rookselect"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:background="@drawable/rookwhite"
                app:layout_constraintLeft_toRightOf="@id/queenselect"
                app:layout_constraintTop_toTopOf="parent"
                />

            <GameCore.SelectButton
                android:id="@+id/bishopselect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bishopwhite"
                app:layout_constraintLeft_toRightOf="@id/rookselect"
                app:layout_constraintTop_toTopOf="parent"
                />

            <GameCore.SelectButton
                android:id="@+id/knightselect"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:background="@drawable/horsewhite"
                app:layout_constraintLeft_toRightOf="@id/bishopselect"
                app:layout_constraintTop_toTopOf="parent"
                />

            <GameCore.SelectButton
                android:id="@+id/keep"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:background="@drawable/pawnwhite"
                app:layout_constraintLeft_toRightOf="@id/knightselect"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </GameCore.Graphics.SelectView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bottommenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@color/menubackground">

        <Button
            android:id="@+id/revert"
            style="@android:style/Widget.Material.Button.Borderless"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ripple"
            android:drawableTop="@drawable/kingwhite"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toLeftOf="@id/redo"
            android:text="Revert" />

        <Button
            android:id="@+id/newgame"
            android:text="New Game"
            android:background="@drawable/ripple"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@android:style/Widget.Material.Button.Borderless"
            android:drawableTop="@drawable/queenwhite"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/redo"
            app:layout_constraintRight_toLeftOf="@id/save_and_export"
            />

        <Button
            android:id="@+id/redo"
            style="@android:style/Widget.Material.Button.Borderless"
            android:drawableTop="@drawable/pawnwhite"
            android:background="@drawable/ripple"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="redo"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/revert"
            app:layout_constraintRight_toLeftOf="@id/newgame"
            />

        <Button
            android:id="@+id/save_and_export"
            style="@android:style/Widget.Material.Button.Borderless"
            android:text="Save"
            android:background="@drawable/ripple"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableTop="@drawable/rookwhite"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@id/newgame"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_gravity="left"
            />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>